  },
  "config": {
    "protocol": "http",
    "webpackProtocol": "",
    "pre-git": {
      "commit-msg": "conventional",
      "pre-commit": [
        "npm test",
        "npm run lint"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  }
}
