    "babel-plugin-react-transform": "^2.0.2",
    "babel-runtime": "^6.23.0",
    "brace": "^0.10.0",
    "chai": "^3.5.0",
    "cssnext-loader": "^1.0.1",
    "eslint": "^3.10.2",
    "eslint-plugin-jsdoc": "^3.0.0",
    "eslint-plugin-react": "^6.7.1",
    "isparta": "^4.0.0",
    "jquery": "^3.2.1",
    "jsdom": "^9.8.3",
    "less": "^2.5.3",
